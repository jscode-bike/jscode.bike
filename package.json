    "@testing-library/user-event": "^7.1.2",
    "monaco-themes": "^0.3.3",
    "prettier": "^2.0.5",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-monaco-editor": "^0.40.0",
